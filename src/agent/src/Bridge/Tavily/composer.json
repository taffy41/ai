{
    "name": "symfony/ai-tavily-tool",
    "description": "Tavily AI tool bridge for Symfony applications.",
    "license": "MIT",
    "type": "symfony-ai-tool",
    "keywords": [
        "ai",
        "bridge",
        "tavily",
        "agent",
        "tool"
    ],
    "authors": [
        {
            "name": "Christopher Hertel",
            "email": "mail@christopher-hertel.de"
        },
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/ai-agent": "@dev",
        "symfony/http-client": "^7.3|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.5.46"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Agent\\Bridge\\Tavily\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\Agent\\Bridge\\Tavily\\Tests\\": "Tests/",
            "Symfony\\AI\\PHPStan\\": "../../../../../.phpstan/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        }
    }
}
